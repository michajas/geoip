{
  "name": "geoip",
  "version": "0.1.0",
  "description": "REST API for IP geolocation data",
  "main": "dist/index.js",
  "scripts": {
    "build": "swc src -d dist",
    "build:scripts": "swc scripts -d dist/scripts",
    "build:all": "yarn build && yarn build:scripts",
    "start": "node dist/index.js",
    "dev": "nodemon --exec swc-node src/index.ts",
    "test": "jest",
    "test:e2e": "jest test/integration --runInBand --forceExit",
    "lint": "eslint src --ext .ts",
    "extract-test-ips": "node load-tests/extract-ips.js",
    "load-test": "k6 run load-tests/geoip-load-test.js",
    "import-csv": "yarn build:all && node dist/scripts/import-csv.js",
    "import-data": "node scripts/direct-import.js",
    "import-full": "node scripts/direct-import.js --clear",
    "update-geoip": "node scripts/direct-import.js -l GeoIP2-City-Locations-en.csv -4 GeoIP2-City-Blocks-IPv4.csv -6 GeoIP2-City-Blocks-IPv6.csv",
    "clear-redis": "node src/scripts/clear-redis.ts",
    "check-redis": "node scripts/check-redis-data.js",
    "test-ip": "node scripts/test-ip-lookup.js",
    "import-direct": "node -r ./scripts/register-ts-paths.js ./src/scripts/import-csv-direct.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "express": "^4.18.2",
    "redis": "^4.6.7",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.72",
    "@swc/jest": "^0.2.27",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.4.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "swc-node": "^1.0.0",
    "testcontainers": "^9.12.0",
    "typescript": "^5.1.6",
    "ts-node": "^10.9.1",
    "ts-jest": "^29.1.1"
  },
  "license": "MIT"
}
